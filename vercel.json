{
  "rewrites": [
    {
      "source": "/",
      "destination": "/orzu-medical/"
    },
    {
      "source": "/orzu-medical/assets/(.*)",
      "destination": "/orzu-medical/assets/$1"
    },
    {
      "source": "/orzu-medical",
      "destination": "/orzu-medical/index.html"
    },
    {
      "source": "/orzu-medical/",
      "destination": "/orzu-medical/index.html"
    },
    {
      "source": "/orzu-medical/:path*",
      "destination": "/orzu-medical/index.html"
    }
  ]
}
